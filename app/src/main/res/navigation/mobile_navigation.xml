<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.navigationv10.ui.home.HomeFragment"
        android:label="Застройщики"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_form"
        android:name="com.example.navigationv10.ui.form.FormFragment"
        android:label="Новый застройщик"
        tools:layout="@layout/fragment_form" />

    <fragment
        android:id="@+id/nav_form_gk"
        android:name="com.example.navigationv10.ui.GKform.GkFormFragment"
        android:label="Новый жилой комплекс"
        tools:layout="@layout/fragment_form_gk" />

    <fragment
        android:id="@+id/nav_form_house"
        android:name="com.example.navigationv10.ui.HouseForm.HouseFormFragment"
        android:label="Новый дом"
        tools:layout="@layout/fragment_house_form" />

    <fragment
        android:id="@+id/nav_form_section"
        android:name="com.example.navigationv10.ui.SectionForm.SectionFormFragment"
        android:label="Новая секция"
        tools:layout="@layout/fragment_section_form" />

    <fragment
        android:id="@+id/nav_form_room"
        android:name="com.example.navigationv10.ui.RoomForm.RoomFormFragment"
        android:label="Новая квартира"
        tools:layout="@layout/fragment_room_form" />

    <fragment
        android:id="@+id/list_gk"
        android:name="com.example.navigationv10.List.GKFragment"
        android:label="Жилой Комплекс"
        tools:layout="@layout/fragment_g_k" />

    <fragment
        android:id="@+id/list_gk_info"
        android:name="com.example.navigationv10.List.GK_Info_Fragment"
        android:label="Информация о ЖК"
        tools:layout="@layout/fragment_g_k__info_" />
    <fragment
        android:id="@+id/list_house"
        android:name="com.example.navigationv10.List.HouseFragment"
        android:label="Дома"
        tools:layout="@layout/fragment_house" />
    <fragment
        android:id="@+id/list_section"
        android:name="com.example.navigationv10.List.SectionFragment"
        android:label="Секции"
        tools:layout="@layout/fragment_section" />
    <fragment
        android:id="@+id/list_room"
        android:name="com.example.navigationv10.List.RoomFragment"
        android:label="Квартиры"
        tools:layout="@layout/fragment_room" />
</navigation>